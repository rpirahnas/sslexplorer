<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
          "struts-config_1_1.dtd">

<struts-config>
	<form-beans/>
	<global-exceptions/>
	<global-forwards/>
	<action-mappings>	
		<action path="/ajaxListAccounts" 
				type="com.sslexplorer.ajax.ListAccountsAction"/>
		<action path="/ajaxListRoles" 
				type="com.sslexplorer.ajax.ListRolesAction"/>
		<action path="/ajaxListAttributeCategories" 
				type="com.sslexplorer.ajax.ListAttributeCategoriesAction"/>
		<action path="/ajaxListValidationStrings" 
				type="com.sslexplorer.ajax.ListValidationStringsAction"/>
		<action path="/ajaxDismissGlobalWarning" 
				type="com.sslexplorer.ajax.DismissGlobalWarningAction"/>
		<action path="/serverRunning" 
				type="com.sslexplorer.ajax.ServerRunningAction"/>
	</action-mappings>
</struts-config>

